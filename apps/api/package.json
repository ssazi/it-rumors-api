{
  "name": "@itrumors/api",
  "version": "0.0.0",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true,
    "tscompiler": "esbuild-register"
  },
  "author": "plain",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "start": "npm run ci && egg-scripts start --sticky --daemon --title=egg-server-itrumors",
    "stop": "egg-scripts stop --title=egg-server-itrumors",
    "dev": "ets && egg-bin dev",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "clean": "tsc -b --clean",
    "check": "npm run lint && npm run cov && npm run tsc",
    "db": "npx sequelize db:migrate",
    "db-prod": "npx sequelize db:migrate --env=production",
    "ets": "ets",
    "db:up": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "@itrumors/types": "workspace:*",
    "cheerio": "1.0.0-rc.12",
    "crypto-js": "^4.1.1",
    "egg": "^3.17.4",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-redis": "^2.5.0",
    "egg-scripts": "^2.17.0",
    "egg-sequelize": "^6.0.0",
    "egg-socket.io": "^4.1.6",
    "egg-ts-helper": "^2.0.0",
    "egg-validate": "^2.0.2",
    "got-cjs": "^12.5.4",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.2",
    "pinyin-pro": "^3.16.2",
    "qcloud-cos-sts": "^3.1.0",
    "svg-captcha": "^1.4.0"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.9",
    "@types/socket.io": "^3.0.2",
    "egg-bin": "^6.4.2",
    "egg-ci": "^2.2.0",
    "esbuild-register": "^3.4.2",
    "sequelize-cli": "^6.6.1",
    "tslib": "^2.6.1"
  },
  "_moduleAliases": {
    "@root": ".",
    "@app": "app",
    "@config": "config"
  }
}
